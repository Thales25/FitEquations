<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9ACBE1"

    android:padding="-10dp"
    tools:context="com.empresa.FitEquantions.MainActivity">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="409dp"
        android:layout_height="264dp"
        android:layout_marginBottom="400dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints">

    </FrameLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.421"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="411dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/noto_serif_bold"
            android:text="          Escolha uma formula:"
            android:textColor="@color/white"
            android:textSize="15dp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="252dp"
        android:layout_height="55dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.276"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.635">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnBenedict"
            android:layout_width="76dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:background="@drawable/button_escolha"
            android:fontFamily="@font/noto_serif_bold"
            android:onClick="telaBenedict"
            android:text="Benedict"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="20dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnMifflin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="53dp"
            android:background="@drawable/button_escolha"
            android:fontFamily="@font/noto_serif_bold"
            android:onClick="telaMifflin"
            android:text="Mifflin"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="20dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="87dp"
        android:layout_height="56dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"

        tools:layout_editor_absoluteX="135dp">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnPollock"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/button_escolha"
            android:fontFamily="@font/noto_serif_bold"
            android:onClick="telaPollock"
            android:text="Pollock\n7"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="20dp" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="178dp"
        android:layout_height="122dp"
        android:layout_gravity="center"
        app:srcCompat="@drawable/imgsegunda_tela"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="-6dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="248dp"
        android:layout_height="31dp"
        android:fontFamily="@font/noto_serif_bold"
        android:text="Percentual de gordura:"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.783"
        tools:layout_editor_absoluteX="38dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
